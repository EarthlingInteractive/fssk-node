{
  "name": "fssk-node-client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 8.9.4"
  },
  "proxy": "http://fssk-node-server:4000",
  "homepage": "/client",
  "scripts": {
    "prestart": "npm install",
    "start": "react-scripts start",
    "prebuild": "npm install",
    "build": "react-scripts build",
    "test": "react-scripts test  --env=jsdom --watchAll",
    "test:ci": "CI=true react-scripts test --env=jsdom --reporters=default --reporters=jest-junit --coverage",
    "eject": "react-scripts eject",
    "lint": "tslint src/**/*.ts{,x}",
    "lint:fix": "tslint src/**/*.ts{,x} --fix"
  },
  "dependencies": {
    "autobind-decorator": "2.1.0",
    "bootstrap": "4.1.3",
    "classnames": "2.2.6",
    "jquery": "3.3.1",
    "mobx": "5.0.3",
    "mobx-react": "5.2.3",
    "moment": "2.22.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.1",
    "reactstrap": "6.3.1",
    "typescript": "3.1.6",
    "validator": "10.4.0",
    "wait-for-expect": "1.0.1"
  },
  "devDependencies": {
    "@types/classnames": "2.2.6",
    "@types/enzyme": "3.1.14",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/fetch-mock": "6.0.3",
    "@types/jest": "23.3.9",
    "@types/node": "10.12.9",
    "@types/react": "16.7.6",
    "@types/react-dom": "16.0.9",
    "@types/react-router-dom": "4.3.0",
    "@types/react-test-renderer": "16.0.1",
    "@types/validator": "9.4.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "fetch-mock": "6.5.2",
    "jest-enzyme": "7.0.0",
    "jest-junit": "5.2.0",
    "react-test-renderer": "16.4.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest-junit": {
    "suiteName": "Client-side jest tests",
    "output": "./build/spec/junit.xml"
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx,ts}"
    ]
  }
}
