{
  "name": "fssk-node-client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">= 8.9.4"
  },
  "proxy": "http://fssk-node-server:4000",
  "homepage": "/client",
  "scripts": {
    "prestart": "npm install",
    "start": "react-scripts-ts start",
    "prebuild": "npm install",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom --watchAll",
    "test:ci": "CI=true react-scripts-ts test --env=jsdom --reporters=default --reporters=jest-junit --coverage",
    "eject": "react-scripts-ts eject",
    "lint": "tslint src/**/*.ts{,x}",
    "lint:fix": "tslint src/**/*.ts{,x} --fix"
  },
  "jest-junit": {
    "suiteName": "Client-side jest tests",
    "output": "./build/spec/junit.xml"
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx,ts}"
    ]
  },
  "dependencies": {
    "autobind-decorator": "2.1.0",
    "bootstrap": "4.1.3",
    "classnames": "2.2.6",
    "jquery": "3.3.1",
    "mobx": "5.0.3",
    "mobx-react": "5.2.3",
    "moment": "2.22.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-router-dom": "4.3.1",
    "react-scripts-ts": "2.16.0",
    "reactstrap": "6.3.1",
    "validator": "10.4.0",
    "wait-for-expect": "1.0.1"
  },
  "devDependencies": {
    "@types/classnames": "2.2.6",
    "@types/enzyme": "3.1.12",
    "@types/enzyme-adapter-react-16": "1.0.2",
    "@types/fetch-mock": "6.0.3",
    "@types/jest": "23.3.1",
    "@types/node": "8.0.47",
    "@types/react": "16.4.7",
    "@types/react-dom": "16.0.6",
    "@types/react-router-dom": "4.3.0",
    "@types/react-test-renderer": "16.0.1",
    "@types/validator": "9.4.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "fetch-mock": "6.5.2",
    "jest-enzyme": "6.0.2",
    "jest-junit": "5.1.0",
    "react-test-renderer": "16.4.1",
    "typescript": "3.0.1"
  }
}
