{
  "name": "fssk-node-server",
  "version": "0.1.0",
  "engines": {
    "node": ">= 8.9.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@types/body-parser": "1.16.7",
    "@types/express": "4.0.39",
    "@types/jest": "21.1.6",
    "@types/node": "8.0.47",
    "@types/passport": "0.4.1",
    "@types/bookshelf": "0.9.7",
    "@types/knex": "0.0.64",
    "@types/validator": "6.3.0",
    "jest": "21.2.1",
    "jest-cli": "21.2.1",
    "nodemon": "1.12.1",
    "ts-jest": "21.2.2",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typescript": "2.5.3"
  },
  "dependencies": {
    "bcrypt": "1.0.3",
    "body-parser": "1.18.2",
    "bookshelf": "0.10.4",
    "connect-session-knex": "1.4.0",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "knex": "0.14.6",
    "morgan": "1.9.0",
    "mustache-express": "1.2.5",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pg": "7.3.0",
    "validator": "9.1.1"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "nodemon",
    "lint": "tslint src/**/*.ts{,x}",
    "lint:fix": "tslint --fix src/**/*.ts{,x}",
    "test": "jest",
    "knex": "knex --knexfile=./src/knexfile.ts",
    "migrate": "npm run knex migrate:latest",
    "migrate:make": "npm run knex migrate:make",
    "migrate:rollback": "npm run knex migrate:rollback",
    "seed": "npm run knex seed:run",
    "seed:make": "npm run knex seed:make"
  }
}
