{
  "name": "fssk-node-server",
  "version": "0.1.0",
  "engines": {
    "node": ">= 8.9.4"
  },
  "jest": {
    "testURL": "http://localhost:4000",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx,ts}"
    ]
  },
  "jest-junit": {
    "suiteName": "Server-side jest tests",
    "output": "./build/spec/junit.xml"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.7",
    "@types/bookshelf": "0.9.9",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.9",
    "@types/knex": "0.14.18",
    "@types/node": "8.0.47",
    "@types/passport": "0.4.6",
    "@types/validator": "9.4.1",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-junit": "5.2.0",
    "nodemon": "1.18.3",
    "ts-jest": "23.0.1",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "3.0.1"
  },
  "dependencies": {
    "bcrypt": "1.0.3",
    "body-parser": "1.18.3",
    "bookshelf": "0.13.3",
    "connect-session-knex": "1.4.0",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "knex": "0.15.2",
    "morgan": "1.9.0",
    "mustache-express": "1.2.7",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pg": "7.4.3",
    "validator": "10.4.0"
  },
  "scripts": {
    "prestart": "npm install",
    "start": "nodemon",
    "lint": "tslint src/**/*.ts{,x}",
    "lint:fix": "tslint --fix src/**/*.ts{,x}",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage",
    "knex": "knex --knexfile=./src/knexfile.ts",
    "migrate": "npm run knex migrate:latest",
    "migrate:make": "npm run knex migrate:make",
    "migrate:rollback": "npm run knex migrate:rollback",
    "seed": "npm run knex seed:run",
    "seed:make": "npm run knex seed:make"
  }
}
