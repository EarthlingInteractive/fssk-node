{
	"extends": "tslint:latest",
	"rules": {
		"curly": true,
		"indent": [true, "tabs"],
		"variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"],
		"no-console": false,
		"no-consecutive-blank-lines": false,
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"no-unused-variable": true
	},
	"jsRules": {
		"curly": true
	}
}
